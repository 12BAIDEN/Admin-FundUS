{
    "version": 2,
    "builds": [
      {
        "src": "api/contact.php",
        "use": "@vercel/php"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "contacts.html",
        "use": "@vercel/static"
      },
      {
        "src": "about.html",
        "use": "@vercel/static"
      },
      {
        "src": "causes.html",
        "use": "@vercel/static"
      },
      {
        "src": "donate.html",
        "use": "@vercel/static"
      },
      {
        "src": "health.html",
        "use": "@vercel/static"
      },
      {
        "src": "service.html",
        "use": "@vercel/static"
      },
      {
        "src": "team.html",
        "use": "@vercel/static"
      },
      {
        "src": "education.html",
        "use": "@vercel/static"
      },
      {
        "src": "basicAmenities.html",
        "use": "@vercel/static"
      },
      {
        "src": "css/*",
        "use": "@vercel/static"
      },
      {
        "src": "scss/*",
        "use": "@vercel/static"
      },
      {
        "src": "lib/*",
        "use": "@vercel/static"
      },
      {
        "src": "img/*",
        "use": "@vercel/static"
      },
      {
        "src": "js/*",
        "use": "@vercel/static"
      }
    ],
    "routes":  [
        { "src": "/api/contacts.php", "dest": "/api/contact.php" },
        { "src": "/contact", "dest": "/contact.html" },
        { "src": "/css/(.*)", "dest": "/css/$1" },
        { "src": "/scss/(.*)", "dest": "/scss/$1" },
        { "src": "/lib/(.*)", "dest": "/lib/$1" },
        { "src": "/img/(.*)", "dest": "/img/$1" },
        { "src": "/js/(.*)", "dest": "/js/$1" },
        { "src": "/(.*)", "dest": "/index.html" }
      ]
  }
  